{
  "name": "react-burger",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://cap-bernardito.github.io/space-burger/",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@ya.praktikum/react-developer-burger-ui-components": "^1.14.0",
    "framer-motion": "^7.6.5",
    "hamburger-react": "^2.5.0",
    "js-cookie": "^3.0.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.4"
  },
  "scripts": {
    "start": "cross-env PUBLIC_URL=/ react-scripts start",
    "start:fd": "cross-env PUBLIC_URL=/ REACT_APP_DATA_SOURCE=fake-data react-scripts start",
    "build": "cross-env PUBLIC_URL=/ react-scripts build",
    "lint:prettier": "prettier --list-different .",
    "lint:js": "eslint --cache --ext \".js,.jsx,.ts,.tsx\" .",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test": "cross-env PUBLIC_URL=/ react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "predeploy": "react-scripts build && cp build/index.html build/404.html",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "sass": "^1.56.0"
  }
}
